#include <iostream>
#include <cstring>
#include <cstdlib>
using namespace std;

int transfer(int byte,short radix){
  int res=0, k=1;
  for(short i=0;byte!=0; ++i){
      res+=(byte%radix)*k;
      k*=10;
      byte/=radix;
  }
  return res;
}

int formout(int out){
   int radix=0,tmp=out;
   while(tmp>0){tmp/=10; ++radix;}
   for(int i=radix; i<8; ++i) cout<<0;
   return out;
}

int main(){
  char IP[30];
  int byte[4];
  cin.getline(IP,30);
  char* digit=strtok(IP,". ");
  for(short i=0;digit!=nullptr;++i){
      byte[i]=atoi(digit);
      digit=strtok(nullptr,". ");
  }
  cout<<"\n";
  cout<<"In BIN(2): ";
  for(short i=0; i<4; ++i) cout<<formout(transfer(byte[i],2))<<".";
  cout<<"\n";
  cout.unsetf(ios::dec); cout.setf(ios::hex);
  cout<<"In HEX(16): ";
  for(short i=0; i<4; ++i) cout<<byte[i]<<".";
  cout<<"\n";
  cout.unsetf(ios::hex); cout.setf(ios::oct);
  cout<<"In OCT(8): ";
  for(short i=0; i<4; ++i) cout<<byte[i]<<".";
  cout.unsetf(ios::oct); cout.setf(ios::dec);
  cout<<"\n";
  if(byte[0]<128){
      if(byte[0]==10 && byte[0]<=126 && byte[1]<256 && byte[2]<256 && byte[3]<256) cout<<"!!!Common adress!!!\n";
      else cout<<"Unic adress\n";
      cout<<"\n";
      cout<<"A class:\n";
      cout<<"\tNet ID = "<<formout(transfer(byte[0],2))<<"\n";
      cout<<"\tHost ID = "<<formout(transfer(byte[1],2))<<"."<<formout(transfer(byte[2],2))<<"."<<formout(transfer(byte[3],2))<<"\n";
  } else if(byte[0]<192){
      if(byte[0]==172 && byte[1]<32 && byte[1]>15 && byte[2]<256 && byte[3]<256) cout<<"!!!Common adress!!!\n";
      else cout<<"Unic adress\n";
      cout<<"\n";
      cout<<"B class:\n";
      cout<<"\tNet ID = "<<formout(transfer(byte[0],2))<<"."<<formout(transfer(byte[1],2))<<"\n";
      cout<<"\tHost ID = "<<formout(transfer(byte[2],2))<<"."<<formout(transfer(byte[3],2))<<"\n";
  } else if(byte[0]<224){
      if(byte[0]==192 && byte[1]==168 && byte[2]<256 && byte[3]<256) cout<<"!!!Common adress!!!\n";
      else cout<<"Unic adress\n";
      cout<<"\n";
      cout<<"C class:\n";
      cout<<"\tNet ID = "<<formout(transfer(byte[0],2))<<"."<<formout(transfer(byte[1],2))<<"."<<formout(transfer(byte[2],2))<<"\n";
      cout<<"\tHost ID = "<<formout(transfer(byte[3],2))<<"\n";
  }else cout<<"\tD/E class";
  return 0;
}
